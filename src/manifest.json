{
  "manifest_version": 3,
  "name": "Custom New Tab",
  "author": "okiscape",
  "homepage_url": "https://github.com/okiscape/custom-new-tab",
  "version": "0.5",
  "description": "Replaces the new tab page with a custom one.",
  "chrome_url_overrides": {
    "newtab": "page/index.html"
  },
  "permissions": [
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "okiscape-newtab@okiscape"
    }
  }
}